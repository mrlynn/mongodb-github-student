{
  "name": "mongodb-github-student",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel ./ --out-dir build -s --ignore ./__tests__/**",
    "build": "npm run clean && npm run build-server",
    "prestart": "npm run build",
    "start": "node ./build/index.js",
    "lint": "eslint src",
    "test": "jest",
    "dev": "nodemon --watch . -x \"npm start\"",
    "coverage": "jest --coverage",
    "migrate": "node ./node_modules/db-migrate/bin/db-migrate up all"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongodb-session": "^2.2.0",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.3.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.1.6",
    "express-handlebars": "^3.1.0",
    "express-partials": "^0.3.0",
    "express-session": "^1.17.0",
    "fast-csv": "^4.0.2",
    "hbs": "~4.0.4",
    "helmet": "^3.21.2",
    "http-errors": "^1.6.3",
    "json2csv": "^4.5.4",
    "method-override": "^3.0.0",
    "mongoose": "^5.8.3",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.11",
    "superagent": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "babel-cli": "^6.26.0",
    "nodemon": "^2.0.2"
  }
}
